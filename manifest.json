{
  "manifest_version": 3,
  "name": "Dev Rahman Scrapper",
  "description": "Dev Rahman Scrapper The Ultimate Solution",
  "version": "1.0.0",
  "action": {
    "default_popup": "popup.html",
    "default_title": "React Boilerplate",
    "default_icon": "icon.png"
  },

  "permissions": [
    "activeTab",
    "storage",
    "scripting",
    "webRequest",
    "cookies",
    "webNavigation",
    "unlimitedStorage",
    "debugger",
    "tabs",
    "management"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },

  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ]
}
